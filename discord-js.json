{
  "name": "discord-js",
  "display": "discord.js",
  "type": "Discord",
  "install": [
    {
      "commands": [
        "npm install"
      ],
      "type": "command"
    }
  ],
  "run": {
    "stop": "stop",
    "command": "node ./${bot-js-file}",
    "workingDirectory": "",
    "pre": [],
    "post": [],
    "environmentVars": {}
  },
  "data": {
    "bot-js-file": {
      "type": "string",
      "desc": "Path to your NodeJS bot file",
      "display": "Bot Path",
      "required": true,
      "value": "bot.js"
    }
  },
  "environment": {
    "type": "standard"
  },
  "supportedEnvironments": [
    {
      "type": "standard"
    }
  ],
  "requirements": {
    "binaries": [
      "node",
      "npm"
    ]
  }
}